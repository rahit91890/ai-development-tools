# AI Development Tools - Environment Configuration
# Copy this file to .env and fill in your actual values

# ============================================
# MLflow Configuration
# ============================================
MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_EXPERIMENT_NAME=ai-development-tools
MLFLOW_ARTIFACT_ROOT=./mlflow_artifacts

# ============================================
# Weights & Biases Configuration
# ============================================
WANDB_API_KEY=your_wandb_api_key_here
WANDB_PROJECT=ai-development-tools
WANDB_ENTITY=your_wandb_username_or_team

# ============================================
# Streamlit Configuration
# ============================================
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_ADDRESS=localhost
STREAMLIT_THEME_PRIMARY_COLOR=#FF4B4B

# ============================================
# Database Configuration (Optional)
# ============================================
DATABASE_URL=sqlite:///./ai_dev_tools.db
# For PostgreSQL: postgresql://username:password@localhost:5432/dbname

# ============================================
# Model Paths and Storage
# ============================================
MODEL_SAVE_PATH=./models
DATA_PATH=./data
LOGS_PATH=./logs

# ============================================
# API Keys for External Services (Optional)
# ============================================
OPENAI_API_KEY=your_openai_api_key_here
HUGGINGFACE_TOKEN=your_huggingface_token_here

# ============================================
# Jupyter Configuration
# ============================================
JUPYTER_PORT=8888
JUPYTER_TOKEN=your_jupyter_token_here

# ============================================
# General Settings
# ============================================
ENVIRONMENT=development
DEBUG=True
LOG_LEVEL=INFO
